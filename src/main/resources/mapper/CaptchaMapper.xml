<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.dao.CaptchaMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.example.model.Captcha">
        <id column="captcha_id" property="captchaId" />
        <result column="session_id" property="sessionId" />
        <result column="captcha" property="captcha" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        captcha_id, session_id, captcha
    </sql>

</mapper>
